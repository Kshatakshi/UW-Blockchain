tState({\r\n        errMessage: err.message\r\n      });\r\n    }\r\n    this.setState({\r\n      loading: false\r\n    });\r\n  };\r\n  render() {\r\n    return (\r\n      <div align=\"center\" className=\"container\" style={{ backgroundImage:`url(\"https://i.pinimg.com/564x/cc/a1/f1/cca1f16b945651b503ccc55b6536aeb5.jpg\")`,height:\"100vh\", backgroundRepeat:\"repeat\"}}>\r\n      <Layout>\r\n        <h1 style={{fontStyle:\"italic\",color:\"Pattensblue\", marginTop:\"40px\"}}>New Request</h1>\r\n        <Card style={{width:\"70vh\", height:\"40vh\", backgroundColor:\"white\",marginTop:\"20px\", paddingRight:\"20px\"}} centered color='teal'>\r\n        <Form className=\"mobile\"  style={{width:\"500px\"}} onSubmit={this.onSubmit} error={!!this.state.errMessage}>\r\n          <Form.Field style={{width:\"350px\", marginLeft:\"10px\", marginTop:\"10px\"}}>\r\n            {/* <label>Description</label> */}\r\n            <Input  style={{ appearance: \"none\", textDecoration: \"none\",color:\"white\", border: \"none\", transition: \"width 0.4s ease-in-out\"}}\r\n              value={this.state.description}  placeholder=\"Name\"\r\n              onChange={event =>\r\n                this.setState({\r\n                  description: event.target.value\r\n                })\r\n              }\r\n            />\r\n          </Form.Field>\r\n          <Form.Field style={{width:\"350px\", marginLeft:\"10px\", marginTop:\"10px\"}}>\r\n            {/* <label>Amount</label> */}\r\n            <Input  style={{ appearance: \"none\", textDecoration: \"none\",color:\"white\", border: \"none\", transition: \"width 0.4s ease-in-out\"}}\r\n              value={this.state.value}  placeholder=\"Name\"\r\n              onChange={event =>\r\n                this.setState({\r\n                  value: event.target.value\r\n                })\r\n              }\r\n              label=\"ether\"\r\n              labelPosition=\"right\"\r\n            />\r\n          </Form.Field>\r\n          <Form.Field  style={{width:\"350px\", marginLeft:\"10px\", marginTop:\"10px\"}}>\r\n            {/* <label>Recipient</label> */}\r\n            <Input  style={{ appearance: \"none\", textDecoration: \"none\",color:\"white\", border: \"none\", transition: \"width 0.4s ease-in-out\"}}\r\n              value={this.state.recipient}  placeholder=\"Name\"\r\n              onChange={event =>\r\n                this.setState({ recipient: event.target.value })\r\n              }\r\n            />\r\n          </Form.Field>\r\n          <Message error>\r\n            <Message.Header>Error</Message.Header>\r\n            <p>{this.state.errMessage}</p>\r\n          </Message>\r\n          <Button  loading={this.state.loading} style={{backgroundColor:'#55BCC9', color:\"white\" ,boxShadow: \"0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19)\"}}>\r\n            Submit\r\n          </Button>\r\n          <Link route= {`/organizations/${this.props.address}/requests`}>\r\n            <a>\r\n              <Button icon labelPosition = \"left\" style={{backgroundColor:'#55BCC9', color:\"white\" ,boxShadow: \"0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19)\"}}><Icon name = \"left arrow\" / >Back</Button>\r\n            </a>\r\n          </Link>\r\n          <style jsx>{`\r\n        @media (max-width: 600px) {\r\n          .mobile {\r\n            width: \"700px\",\r\n            paddingBottom: \"80px\",\r\n  paddingLeft:\"80px\",\r\n  border: \"2px grey\",\r\n  margin: \"0\" ,\r\n  alignContent:\"center\"\r\n\r\n          }\r\n        }\r\n      `}</style>\r\n        </Form>\r\n        </Card>\r\n      </Layout>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default NewRequest;\r\n"]},"metadata":{},"sourceType":"module"}